from snakemake.utils import min_version
min_version("8.0")

import os

configfile: "../config/config.yaml"
SAMPLES, = glob_wildcards(os.path.join(config["reads"],"{sample}_1.fastq.gz"))

include: "rules/roundAB"

rule all:
    input:
        expand(os.path.join(config["outdir"], "{sample}", "phage_analysis", "done"), sample = SAMPLES)
    default_target: True
